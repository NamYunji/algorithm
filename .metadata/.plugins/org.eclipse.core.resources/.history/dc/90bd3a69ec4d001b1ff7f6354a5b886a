package step3_For;

import java.util.Scanner;

public class _9 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("별 개수 입력 : ");
		int n = scanner.nextInt();
		if (n >= 1 && n <= 100)
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					System.out.print("*");
				}
			}
		else
		System.out.println("A는 0보다 크고, B는 10보다 작아야 합니다.");
		}
	}

